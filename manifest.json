{
    "manifest_version": 2,
    "name": "Check add-on compatibility with next ESR.",
    "version": "1.1",
    "author": "opto (Klaus Buecher)",
    "applications": {
        "gecko": {
            "id": "addoncompat.check@opto.one",
            "strict_min_version": "78.0",
            "strict_max_version": "91.0"
        }
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_title": "Check add-on updates and compatibility in next ESR",
        "default_label": ""
    },


     	

	"options_ui": {
		"page": "./popup/tt/ttt/options.html",
    "browser_style": true,
    "open_in_tab": true
	  },
	  

      
    "permissions": [
        "management", "activeTab", "tabs", "tabHide", "clipboardWrite"
    ]
}
